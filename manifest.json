{
    "manifest_version": 2,
    "name": "Eximus Extension",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": ["http://eximusgames.com/*"],
          //"css": ["styles.css"],
          "js": ["scripts/content.js"]
        }
      ],
      "background": {"scripts": ["scripts/background.js"]},
      "browser_action": {
          "default_title": "Eximus",
          "default_icon": "images/icon.png",
          "default_popup": "popup/index.html"
      }
      
}